                <li class="column-vehicle-item" style="--bri-anim-idx: 0;" id="brand-nav-vm-av-seg5-explorer-item">
                    <div class="vehicle-tile active-veh-1">
                        
                        <div class="vehicle-tile-inner veh-item-1" data-link-context="{ &#34;model&#34;:&#34;Explorer&#34;, &#34;year&#34;:&#34;2025&#34;, &#34;trim&#34;:&#34;st&#34; }" data-pricing-context="{ &#34;model&#34;:&#34;Explorer&#34;, &#34;year&#34;:&#34;2025&#34;, &#34;trimId&#34;:&#34;st&#34; }">
