{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "US Truck Dataset",
  "type": "object",
  "properties": {
    "make": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "model_year": {
      "type": "integer",
      "minimum": 1990,
      "maximum": 2050
    },
    "fuel": {
      "type": "string"
    },
    "payload_lb": {
      "type": "number",
      "minimum": 0
    },
    "market": {
      "type": "string",
      "enum": [
        "US",
        "non-US"
      ]
    },
    "notes": {
      "type": "string"
    },
    "schema_version": {
      "type": "string"
    },
    "uid": {
      "type": "string"
    }
  },
  "required": [
    "make",
    "model",
    "model_year",
    "uid",
    "market",
    "schema_version"
  ]
}